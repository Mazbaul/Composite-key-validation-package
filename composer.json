
{
    "name": "mazbaul/comp",
    "description": "Custom Laravel Validator For Composite Key Validation",
    "keywords": ["laravel"],
    "homepage": "https://github.com/Mazbaul/comp",
    "license": "MIT",
    "authors": [
        {
            "name": "Mazbaul Alam",
            "email": "mazbaul17@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.*|^6.0",
        "illuminate/validation": "5.*|^6.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.0",
        "bossa/phpspec2-expect": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "Mazbaul\\Comp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mazbaul\\Comp\\CompServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
